@model Projet_Web_Commerce.Models.PPGestionnaire

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Create</title>
</head>
<body>

<h4>PPGestionnaire</h4>
