@{
    ViewData["Title"] = "Home Page";
}
@using Microsoft.AspNetCore.Identity
@inject SignInManager<Projet_Web_Commerce.Areas.Identity.Data.Utilisateur> SignInManager

@if (SignInManager.IsSignedIn(User))
{
    <head>
        <meta http-equiv="refresh" content="0; url=https://localhost:7267/Films" />
    </head>
}
else
{
    <head>
        <meta http-equiv="refresh" content="0; url=https://localhost:44376/Identity/Account/Login" />
    </head>
}
